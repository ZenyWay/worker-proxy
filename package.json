{
  "name": "worker-proxy",
  "version": "1.2.0",
  "description": "thread-local proxy for a given service spawned in dedicated Worker thread",
  "main": "dist",
  "browser": "dist/bundle.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "mkdirp dist && rimraf dist/*",
    "build": "tsc -p src && npm run bundle",
    "bundle": "browserify dist/index.js > dist/bundle.js",
    "example": "npm run example:bundle && npm run example:serve",
    "example:serve": "http-server -c-1 -i -e html -o -p 6510 spec/example/dist",
    "preexample:bundle": "mkdirp spec/example/dist && rimraf spec/example/dist/* && cpx spec/example/src/index.html spec/example/dist",
    "example:bundle": "npm run example:bundle:index & npm run example:bundle:worker",
    "example:bundle:index": "browserify spec/example/src/index.ts -p [ tsify --project spec/example/src ] -o spec/example/dist/index.js",
    "example:bundle:worker": "browserify spec/example/src/worker.ts -p [ tsify --project spec/example/src ] -o spec/example/dist/worker.js",
    "pretest": "mkdirp spec/reports && rimraf spec/reports/*",
    "test": "npm run test:proxy && npm run test:worker",
    "test:proxy": "karma start spec/proxy/karma.conf.js --single-run",
    "test:worker": "karma start spec/worker/karma.conf.js --single-run",
    "test:debug:proxy": "karma start spec/proxy/karma.conf.js --no-single-run",
    "test:debug:worker": "karma start spec/worker/karma.conf.js --no-single-run",
    "test:coverage": "npm run test:coverage:proxy && npm run test:coverage:worker && npm run test:coverage:serve",
    "test:coverage:serve": "http-server -c-1 -o -p 6510 spec/reports/coverage",
    "test:coverage:proxy": "rimraf spec/reports/coverage/proxy && karma start spec/proxy/karma-coverage.conf.js --single-run && remap-istanbul -i spec/reports/coverage/proxy/chrom*/coverage.json -o spec/reports/coverage/proxy -t html",
    "test:coverage:worker": "rimraf spec/reports/coverage/worker && karma start spec/worker/karma-coverage.conf.js --single-run && remap-istanbul -i spec/reports/coverage/worker/chrom*/coverage.json -o spec/reports/coverage/worker -t html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyway/worker-proxy.git"
  },
  "keywords": [
    "worker",
    "proxy"
  ],
  "author": {
    "name": "Stephane M. Catala",
    "email": "stephane@zenyway.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/zenyway/worker-proxy/issues"
  },
  "homepage": "https://github.com/zenyway/worker-proxy#readme",
  "dependencies": {
    "@types/bluebird": "^2.0.29",
    "@types/debug": "0.0.28",
    "bluebird": "^3.4.0",
    "debug": "^2.2.0",
    "tslib": "^1.0.0",
    "webworkify": "^1.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "browserify": "^13.1.0",
    "browserify-istanbul": "^2.0.0",
    "concurrently": "^2.1.0",
    "cpx": "^1.3.2",
    "eslint": "^2.13.1",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.1",
    "karma-junit-reporter": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mkdirp": "^0.5.1",
    "proxyquire": "^1.7.9",
    "proxyquire-universal": "^1.0.8",
    "proxyquireify": "^3.2.0",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.4",
    "tsify": "^1.0.1",
    "typescript": "^2.0.0",
    "watchify": "^3.7.0"
  }
}
